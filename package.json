{
  "name": "staroverlay-backend",
  "version": "0.1.2",
  "description": "",
  "author": {
    "name": "Sammwy",
    "url": "https://github.com/sammwyy"
  },
  "private": true,
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev:nowatch": "cross-env NODE_ENV=development nest start",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start": "cross-env NODE_ENV=production node .",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/websockets": "^10.3.0",
    "@twurple/api": "^7.0.7",
    "@twurple/auth": "^7.0.7",
    "@twurple/eventsub-base": "^7.0.7",
    "@twurple/eventsub-ws": "^7.0.7",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "geoip-lite": "^1.4.9",
    "graphql": "^16.6.0",
    "mongoose": "^8.4.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-twitch": "^1.0.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.2.0",
    "socket.io": "^4.7.4",
    "twitch-api-ts": "^0.1.0",
    "twitch-fetcher": "^1.2.1",
    "twitch-oauth": "0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.2",
    "@commitlint/config-conventional": "^19.0.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/geoip-lite": "^1.4.4",
    "@types/jest": "29.5.12",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "prettier": "^3.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
